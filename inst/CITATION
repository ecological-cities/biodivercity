citHeader("To cite biodivercity in publications use:")

citEntry(
  entry    = "Misc",
  title    = "biodivercity: An R package to assess the diversity of animal species in urban areas",
  author   = " ",
  journal  = " ",
  year     = " ",
  volume   = " ",
  number   = " ",
  pages    = "",
  url      = "https://github.com/ecological-cities/biodivercity",
  doi      = " ",
  textVersion = paste("biodivercity: An R package to assess the diversity of animal species in urban areas. https://github.com/ecological-cities/biodivercity.")
)
